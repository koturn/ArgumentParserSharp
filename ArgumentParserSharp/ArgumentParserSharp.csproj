<Project Sdk="Microsoft.NET.Sdk" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net6.0;net8.0</TargetFrameworks>
    <LangVersion>13</LangVersion>
    <Copyright>Copyright (C) 2020 koturn All Rights Reserverd.</Copyright>
    <Authors>koturn</Authors>
    <Company>koturn</Company>
    <Description>Command Line Argument Parser for .NET.</Description>
    <PackageProjectUrl>https://github.com/koturn/ArgumentParserSharp</PackageProjectUrl>
    <PackageLicenseFile>..\LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/koturn/ArgumentParserSharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Nullable>enable</Nullable>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Version>1.1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="_Compatible\" />
  </ItemGroup>

</Project>
